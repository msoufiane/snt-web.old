{
  "name": "snt-web",
  "version": "0.1.0",
  "description": "sentAd web app",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "babel-node tools/setup-hooks.js",

    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "test:watch": "npm run test -- --watchAll",
    "open:src": "babel-node tools/srcServer.js",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "lint": "esw webpack.config.* src tools --color",

    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",

    "publish:dev": "babel-node tools/s3-upload.js dev",
    "publish:pre": "babel-node tools/s3-upload.js pre",
    "publish:pro": "babel-node tools/s3-upload.js pro",

    "cover": "cat ./coverage/lcov.info | node_modules/.bin/codacy-coverage -t 7a7d6515c73d4bc8ac81c169d3b8b8ac -v",

    "test:CI": "jest --coverage",

    "start:dist": "yarn run build && babel-node tools/distServer.js",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Soufiane Mghanen <mgh.soufiane@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "admin-lte": "2.4.2",
    "axios": "0.16.2",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "6.26.0",
    "express": "^4.16.2",
    "ionicons": "3.0.0",
    "object-assign": "4.1.0",
    "path": "^0.12.7",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.6.0",
    "redux-form": "7.1.1",
    "redux-saga": "0.15.6"
  },
  "devDependencies": {
    "autoprefixer": "7.1.4",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.6",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "1.6.0",
